			    format(str2,sizeof(str2),"%d L",VehicleFuel[vehicleid]);
			    PlayerTextDrawSetString(i,PlayerSp[i][30],str2);
			    
			    format(str2,sizeof(str2),"%d km/h",GetSpeedKM(i));
			    PlayerTextDrawSetString(i,PlayerSp[i][29],str2);
			
