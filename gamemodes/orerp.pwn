stock Converts(number)
{
	new hours = 0, mins = 0, secs = 0, stringl[30];
	hours = floatround(number / 3600);
	mins = floatround((number / 60) - (hours * 60));
	secs = floatround(number - ((hours * 3600) + (mins * 60)));
	if(hours > 0)
	{
		format(stringl, 30, "%d:%02d:%02d", hours, mins, secs);
	}
	else
	{
		format(stringl, 30, "%d:%02d", mins, secs);
	}
	return stringl;
}
