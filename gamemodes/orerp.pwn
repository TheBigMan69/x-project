						SendClientMessage(i,0x0099FFAA, "Этому игроку выдано вознаграждение в размере {00cc00}20.000$");
					}
					if(target == INVALID_PLAYER_ID)
					{
					    new upfive[80];
						format(upfive, sizeof(upfive), "SELECT * FROM `accounts` WHERE name = '%s'", PlayerInfo[i][pAward]);
						new Cache: ramil_jopa = mysql_query(dbHandle,upfive,true);
						if(cache_num_rows() > 0)
						{
						    new getnamers[100];
							new setplus,rows,temp[40];
							format(getnamers,sizeof(getnamers), "SELECT money FROM `accounts` WHERE name = '%s' LIMIT 1",PlayerInfo[i][pAward]);
							new Cache: lolpizda = mysql_query(dbHandle,getnamers,true);
