public PlayerUpdate(i)
{
	if(IsPlayerNPC(i)) return 1;
	if(!IsPlayerConnected(i)) return true;
	if(IsPlayerInAnyVehicle(i))
	{
		if(!IsAVelik(i))
		{
			if(GetPlayerState(i) == 2)
			{
			    new vehicleid = GetPlayerVehicleID(i);
			    new maxxx,str2[50];
			    new fu = floatround(VehicleFuel[vehicleid]);
			    switch(fu){
			        case 0: maxxx = 0;
					case 1..10: maxxx = 1;
					case 11..20: maxxx = 2;
					case 21..30: maxxx = 3;
					case 31..40: maxxx = 4;
					case 41..50: maxxx = 5;
					default: maxxx = 6;
				}
				new Float:ms;
				ms = (GetSpeedKM(i)/2.7)/10000;
			    Probeg[vehicleid] += ms;
				for(new t; t < 6; t++){
				    if(maxxx > t) PlayerTextDrawBoxColor(i,PlayerSp[i][t+2], 0xFF9900FF);
				    else PlayerTextDrawBoxColor(i,PlayerSp[i][t+2], -1);
				    PlayerTextDrawShow(i,PlayerSp[i][t+2]);
				}
			    switch(GetSpeedKM(i)){
			        case 0: maxxx = 0;
			        case 1..10: maxxx = 1;
					case 11..20: maxxx = 2;
					case 21..30: maxxx = 3;
					case 31..40: maxxx = 4;
					case 41..50: maxxx = 5;
			        case 51..60: maxxx = 6;
					case 61..70: maxxx = 7;
					case 71..80: maxxx = 8;
					case 81..90: maxxx = 9;
					case 91..100: maxxx = 10;
					case 101..110: maxxx = 11;
					case 111..120: maxxx = 12;
					case 121..130: maxxx = 13;
					case 131..140: maxxx = 14;
					default: maxxx = 15;
				}
				for(new t; t < 15; t++){
				    if(maxxx > t) PlayerTextDrawBoxColor(i,PlayerSp[i][t+9], 0xFF9900FF);
				    else PlayerTextDrawBoxColor(i,PlayerSp[i][t+9], -1);
				    PlayerTextDrawShow(i,PlayerSp[i][t+9]);
