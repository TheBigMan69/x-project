stock UpdateHouse(h)
{
		DestroyDynamicMapIcon(HouseInfo[h][hMIcon]);
		DestroyPickup(HouseInfo[h][hPickup]);
		if(HouseInfo[h][hOwned] == 0)
		{
			HouseInfo[h][hPickup] = CreatePickup(1273, 23, HouseInfo[h][ExitX], HouseInfo[h][ExitY], HouseInfo[h][ExitZ],-1);
			HouseInfo[h][hMIcon] = CreateDynamicMapIcon(HouseInfo[h][ExitX], HouseInfo[h][ExitY], HouseInfo[h][ExitZ], 31, -1, 0, -1, -1, 70.0);
		}
		if(HouseInfo[h][hOwned] == 1)
		{
			HouseInfo[h][hPickup] = CreatePickup(1272, 23, HouseInfo[h][ExitX], HouseInfo[h][ExitY], HouseInfo[h][ExitZ],-1);
			HouseInfo[h][hMIcon] = CreateDynamicMapIcon(HouseInfo[h][ExitX], HouseInfo[h][ExitY], HouseInfo[h][ExitZ], 32, -1, 0, -1, -1, 70.0);
		}
		return true;
}
stock GetHouseZone(houid)
{
	new zone[32],bool:getzone;
	for(new i=0; i<sizeof(gZoneInfo); i++)
	{
		if(HouseInfo[houid][ExitX] >= gZoneInfo[i][SAZONE_AREA][0] && HouseInfo[houid][ExitY] <= gZoneInfo[i][SAZONE_AREA][3]
				&& HouseInfo[houid][ExitX] >= gZoneInfo[i][SAZONE_AREA][1] && HouseInfo[houid][ExitY] <= gZoneInfo[i][SAZONE_AREA][4])
		{
			settext(zone, gZoneInfo[i][SAZONE_NAME]);
			getzone = true;
			break;
		}
	}
	if(!getzone) zone = "Неизвестно";
	return zone;
}

