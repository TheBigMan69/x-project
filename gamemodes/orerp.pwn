public OnGameModeExit()
{
    KillTimer(mtimer);
	SaveStoreGrove();
	SaveStoreBallas();
	SaveStoreAztec();
	SaveStoreVagos();
	SaveStoreRifa();
	SaveGangsStores();
	SaveMafiaStores();
	SaveArmyStores();
	SaveMiner();

	foreach(new a: Player)
	{
		if(IsPlayerConnected(a))
		{
			if(Logged[a] == true)
			{
				OnPlayerSave(a);
			}
		}
	}
	if(loadfs == true)
	{
		mysql_query(dbHandle,"DELETE FROM election WHERE 1",false);
		mysql_query(dbHandle,"TRUNCATE TABLE `election`",false);
		Delete3DTextLabel(TextAS[0]);
		Delete3DTextLabel(TextAS[1]);
		Delete3DTextLabel(TextAS[2]);
		DestroyPickup(bulleten1);
		DestroyPickup(bull1);
		DestroyPickup(bull2);
		DestroyActor(bullact[0]);
		DestroyActor(bullact[1]);
		DestroyActor(bullact[2]);
		DestroyActor(bullact[3]);
		DestroyActor(bullact[4]);
		DestroyActor(bullact[5]);
		loadfs = false;
		votego = false;
		votestart = false;
	}
	mysql_close();
	return true;
}

