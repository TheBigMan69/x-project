public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
    if(newkeys & KEY_ACTION)
	{
		if(!IsAVelik(playerid))
		{
			if(GetPlayerState(playerid) != 2) return 0;
			new vid = GetPlayerVehicleID(playerid);
			if(VehicleFuel[vid] <= 0)
			{
				VehicleFuel[vid] = 0;
				GameTextForPlayer(playerid, "~r~No Fuel", 1000, 4);
				SetVehicleParamsEx(vid,0,VehInfo[vid][pLights],VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
				return true;
			}
			GetVehicleParamsEx(vid,VehInfo[vid][pEngine],VehInfo[vid][pLights],VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
			if(VehInfo[vid][pEngine] == 0) SetVehicleParamsEx(vid,1,VehInfo[vid][pLights],VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
			if(VehInfo[vid][pEngine] == 1) SetVehicleParamsEx(vid,0,VehInfo[vid][pLights],VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
			if(VehInfo[vid][pEngine] == 0) VehInfo[vid][pEngine] = 1;
			if(VehInfo[vid][pEngine] == 1) VehInfo[vid][pEngine] = 0;
		}
	}
	if ( GetPlayerState ( playerid ) == PLAYER_STATE_ONFOOT )
    {
        if ( newkeys == KEY_WALK )
        {
           // if ( GetPlayerInterior ( playerid ) != 1 ) return 1 ; // у Калигулы 1 id интерьера
            if ( GetPVarInt ( playerid, "p_sm_used" ) != 0 ) return 1 ; // Проверяем использует уже игрок какую-либо слот-машину или нет
            for ( new sm = 0; sm < sm_count ; sm ++ )
            {
                if ( sm_used { sm } != 0 ) continue ; // Проверяем занята слот-машина или нет
                if ( ! IsPlayerInRangeOfPoint ( playerid, 1, sm_coord [ sm ] [ 0 ], sm_coord [ sm ] [ 1 ], sm_coord [ sm ] [ 2 ] ) ) continue ; // Проверяем на нахождение в площади активации слот-машины
                SetPVarInt ( playerid, "p_sm_used", sm + 1 ) ;
                TogglePlayerControllable ( playerid, false ) ;

                sm_used { sm } = 1 ;

                sm_ptd [ playerid ]  = CreatePlayerTextDraw ( playerid, 250.000000, 277.000000, "BET:  0$" ) ;
                PlayerTextDrawBackgroundColor ( playerid, sm_ptd [ playerid ] , 255 ) ;
                PlayerTextDrawFont ( playerid, sm_ptd [ playerid ] , 2 ) ;
                PlayerTextDrawLetterSize ( playerid, sm_ptd [ playerid ] , 0.310000, 2.700000 ) ;
                PlayerTextDrawColor ( playerid, sm_ptd [ playerid ] , -2711041 ) ;
                PlayerTextDrawSetOutline ( playerid, sm_ptd [ playerid ] , 1 ) ;
                PlayerTextDrawSetProportional ( playerid, sm_ptd [ playerid ] , 1 ) ;
                PlayerTextDrawSetSelectable ( playerid, sm_ptd [ playerid ] , 0 ) ;

                for ( new i = 0 ; i < 4 ; i ++ )
                {
                    TextDrawShowForPlayer ( playerid, sm_td [ i ] ) ;
                }
                PlayerTextDrawShow ( playerid, sm_ptd [ playerid ] ) ;
                SelectTextDraw ( playerid, 0xFFDA82FF ) ;
                if ( sm == 0 ) SetPlayerCameraPos(playerid, 1117.799438, -4.366270, 1001.012390),SetPlayerCameraLookAt(playerid, 1112.926269, -4.280145, 999.896850);//1,11554
                if ( sm == 1 ) SetPlayerCameraPos(playerid, 1117.799438, -3.512000, 1001.009390),SetPlayerCameraLookAt(playerid, 1112.926269, -3.431000, 999.867850);
                if ( sm == 2 ) SetPlayerCameraPos(playerid, 1117.810000, -2.672000, 1001.009390),SetPlayerCameraLookAt(playerid, 1112.930000, -2.591000, 999.867850);
                if ( sm == 3 ) SetPlayerCameraPos(playerid, 1117.830000, -0.466000, 1001.009390),SetPlayerCameraLookAt(playerid, 1112.950000, -0.385000, 999.867850);
                if ( sm == 4 ) SetPlayerCameraPos(playerid, 1117.832000, 0.422000, 1001.009390),SetPlayerCameraLookAt(playerid, 1112.952000, 0.341000, 999.867850);
                if ( sm == 5 ) SetPlayerCameraPos(playerid, 1117.830000, 1.246000, 1001.009390),SetPlayerCameraLookAt(playerid, 1112.950000, 1.164000, 999.867850);//
                break ;
            }
        }
    }
	if(IsKeyJustDown(16,newkeys,oldkeys))
	{
		for(new cars=0;cars<MAX_VEHICLES;cars++)
        {
			if(GetDistanceToCar(playerid,cars)<1)
			{
				if(GetVehicleModel(cars)==449) PutPlayerInVehicle(playerid,cars,0),TogglePlayerSpectating(playerid,0);
			}
		}
	}
	if(takingselfie[playerid] == 1)
	{
		if(PRESSED(KEY_ANALOG_RIGHT))
		{
			GetPlayerPos(playerid,lX[playerid],lY[playerid],lZ[playerid]);
			static Float: n1X, Float: n1Y;
		    if(Degree[playerid] >= 360) Degree[playerid] = 0;
		    Degree[playerid] += Speed;
		    n1X = lX[playerid] + Radius * floatcos(Degree[playerid], degrees);
		    n1Y = lY[playerid] + Radius * floatsin(Degree[playerid], degrees);
		    SetPlayerCameraPos(playerid, n1X, n1Y, lZ[playerid] + Height);
		    SetPlayerCameraLookAt(playerid, lX[playerid], lY[playerid], lZ[playerid]+1);
		    SetPlayerFacingAngle(playerid, Degree[playerid] - 90.0);
		}
		if(PRESSED(KEY_ANALOG_LEFT))
		{
		    GetPlayerPos(playerid,lX[playerid],lY[playerid],lZ[playerid]);
			static Float: n1X, Float: n1Y;
		    if(Degree[playerid] >= 360) Degree[playerid] = 0;
		    Degree[playerid] -= Speed;
		    n1X = lX[playerid] + Radius * floatcos(Degree[playerid], degrees);
		    n1Y = lY[playerid] + Radius * floatsin(Degree[playerid], degrees);
		    SetPlayerCameraPos(playerid, n1X, n1Y, lZ[playerid] + Height);
		    SetPlayerCameraLookAt(playerid, lX[playerid], lY[playerid], lZ[playerid]+1);
		    SetPlayerFacingAngle(playerid, Degree[playerid] - 90.0);
		}
	}
    if(newkeys & 1024)
	{
		if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
		{
			if(GetPlayerState(playerid) == 2)
			{
				new vid = GetPlayerVehicleID(playerid);
				GetVehicleParamsEx(vid,VehInfo[vid][pEngine],VehInfo[vid][pLights],VehInfo[vid][pAlarm],VehInfo[vid][pDoors],VehInfo[vid][pBonnet],VehInfo[vid][pBoot],VehInfo[vid][pObjective]);
